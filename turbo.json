{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "test": {
      "dependsOn": [
        "^build"
      ],
      "outputs": []
    },
    "preview": {
      "dependsOn": [
        "build"
      ]
    },
    "dev": {
      "cache": false,
      "dependsOn": [
        "build"
      ]
    },
    "lint": {
      "outputs": []
    },
    "prettier-fix": {
      "outputs": []
    },
    "start": {
      "cache": false
    },
    "types": {
      "outputs": [
        "dist/**/*.d.ts"
      ]
    }
  }
}